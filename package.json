{
  "name": "node-graphql-boilerplate",
  "version": "1.0.17",
  "description": "Boilerplate nodejs backend",
  "license": "MIT",
  "author": "Jakub Å vehla",
  "main": "src/main.ts",
  "scripts": {
    "build:ts-assets": "rm -rf dist-minified && ts-node build-scripts/buildClientAssets",
    "prepare:assets:stage-1": "npm run build:ts-assets && cp .env.stage-1.env dist-minified/.env",
    "prepare:assets:production": "npm run build:ts-assets && cp .env.production.env dist-minified/.env",
    "terraform:apply": "cd terraform && terraform apply",
    "deploy:infrastructure": "npm run prepare:assets:production && npm run terraform:apply",
    "deploy:code:production": "npm run prepare:assets:production && ./deploy-scripts/production-lambda-code.sh",
    "deploy:code:stage-1": "npm run prepare:assets:stage-1 && ./deploy-scripts/stage-1-lambda-code.sh",
    "t:a": "npm run terraform:apply",
    "d:i": "npm run deploy:infrastructure",
    "d:c:p": "npm run deploy:code:production",
    "d:c:s1": "npm run deploy:code:stage-1",
    "db:init-schema": "ts-node ./scripts/createDbCLI",
    "db:load-data": "ts-node ./scripts/loadDataCLI",
    "db:hard-init": "npm run db:init-schema && npm run db:load-data",
    "docker:db:hard-init": "docker-compose run app npm run db:hard-init",
    "docker:db:up": "docker-compose up db",
    "docker:dev": "docker-compose up --build",
    "docker:stop": "docker-compose stop",
    "docker:install": "docker-compose run --no-deps --rm app npm i",
    "dev": "nodemon --watch 'src/**/*.ts' --ignore 'src/**/*.spec.ts' --exec 'ts-node' src/index.ts",
    "dev:EPIPE:ERROR": "ts-node-dev --respawn --transpile-only --exit-child --ignore-watch node_modules src/index.ts",
    "kill:2020": "kill $(lsof -ti:2020)",
    "deprecated:dev": "ts-node-dev --respawn --transpile-only --ignore-watch node_modules src/index.ts",
    "l": "yarn run lint:fix",
    "lint": "eslint \"{src,fixtures,scripts,dbMigrationsScripts,tests}/**/*.{js,ts}\"",
    "lint:fix": "eslint \"{src,fixtures,scripts,dbMigrationsScripts,tests}/**/*.{js,ts}\" --fix",
    "ts:check-types": "tsc",
    "ts:check-types:watch": "tsc --watch",
    "test": "jest"
  },
  "dependencies": {
    "apollo-errors": "^1.9.0",
    "apollo-server-express": "^2.21.1",
    "apollo-tracing": "^0.12.2",
    "aws-sdk": "^2.874.0",
    "axios": "^0.21.1",
    "bcryptjs": "^2.4.3",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "dataloader": "^2.0.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "express-graphql": "^0.12.0",
    "google-auth-library": "^7.0.2",
    "graphql": "^15.4.0",
    "graphql-custom-types": "^1.6.0",
    "graphql-playground-middleware-express": "^1.7.22",
    "jsonwebtoken": "^8.5.1",
    "knex": "^0.95.1",
    "nodemailer": "^6.5.0",
    "nodemon": "^2.0.7",
    "pg": "^8.5.1",
    "postonents": "^0.4.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "reflect-metadata": "^0.1.13",
    "serverless-http": "^2.7.0",
    "typed-env-parser": "^0.1.0",
    "typeorm": "^0.2.31",
    "url-join": "^4.0.1",
    "yup": "^0.32.9"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.10",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.11",
    "@types/express-session": "^1.17.3",
    "@types/jest": "^26.0.22",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/node": "^14.14.34",
    "@types/nodemailer": "^6.4.0",
    "@types/pg": "^7.14.11",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.2",
    "@types/terser-webpack-plugin": "^5.0.2",
    "@types/url-join": "^4.0.0",
    "@types/webpack": "^4.41.26",
    "@typescript-eslint/eslint-plugin": "^4.14.0",
    "@typescript-eslint/parser": "^4.17.0",
    "awesome-typescript-loader": "^5.2.1",
    "eslint": "^7.22.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-sort-imports-es6-autofix": "^0.5.0",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "terser-webpack-plugin": "^5.1.1",
    "ts-jest": "^26.5.4",
    "ts-loader": "^8.0.18",
    "ts-node": "^9.1.1",
    "ts-node-dev": "^1.1.1",
    "typescript": "^4.2.3",
    "webpack": "^5.25.0",
    "webpack-cli": "^4.5.0"
  }
}
